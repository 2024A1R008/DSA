#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define size 100
int tree1[size],tree2[size];
void init(int* tree){
    for(int i=0;i<size;i++)
        tree[i]=-1;
}
void insert(int* tree,int index,int value){
    if(tree[index]==-1){ 
        tree[index]=value;
        return;}
    else if(value<tree[index])
        insert(tree,2*index+1,value);
    else
        insert(tree,2*index+2,value);
}
int identical(int* tree1,int* tree2){
    for(int i=0;i<size;i++){
        if(tree1[i]!=tree2[i])
            return 0;
    }
    return 1;
}
int main() {
    int n;
    scanf("%d", &n);
    int a[n],b[n];
    for(int i=0;i<n;i++){
        scanf("%d", &a[i]);
    }
    for(int i=0;i<n;i++){
        scanf("%d", &b[i]);
    }
    init(tree1); init(tree2);
    for(int i=0;i<n;i++){
        insert(tree1,0,a[i]);
    }
    for(int i=0;i<n;i++){
        insert(tree2,0,b[i]);
    }
    if(identical(tree1,tree2)){
        printf("Identical");
    }
    else{
        printf("Not Identical");
    }
    return 0;
}
